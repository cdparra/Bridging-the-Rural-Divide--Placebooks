<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/linearLayoutPage"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:orientation="vertical"
  android:gravity="center_horizontal"
  android:padding="12dp"
  android:background="@+drawable/book_background"
  >
  
  
  <TextView android:id="@+id/pageText" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#000000" android:textSize="4dp" />
  <WebView android:id="@+id/pageWeb" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_gravity="center_horizontal"/>
</RelativeLayout>
