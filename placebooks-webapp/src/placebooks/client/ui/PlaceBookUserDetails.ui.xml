<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res' type='placebooks.client.resources.Resources'/>		
	<g:FlowPanel>
		<g:Label ui:field="userName"></g:Label>
		<g:SimplePanel><g:Anchor ui:field="everytrail">Link Everytrail Account</g:Anchor></g:SimplePanel>
		<g:SimplePanel><g:Anchor ui:field="logout">Logout</g:Anchor></g:SimplePanel>
		<g:FlowPanel ui:field="syncPanel"><g:Image resource='{res.progress3}' /><g:InlineLabel>Syncing with Everytrail</g:InlineLabel></g:FlowPanel>		
	</g:FlowPanel>
</ui:UiBinder> 