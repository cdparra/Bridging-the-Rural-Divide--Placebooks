<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel>
		<g:Label ui:field="infoLabel" />
		<g:FormPanel ui:field="form"> 
			<g:FlowPanel>
				<g:FileUpload ui:field="upload" />
				<g:Hidden ui:field="itemKey" name="itemKey" />
				<g:Label>Copyright Information:</g:Label>
				<g:TextArea ui:field="copyright"/>
				<g:FlowPanel>
					<g:SubmitButton ui:field="uploadButton">Upload</g:SubmitButton>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FormPanel>
	</g:FlowPanel>
</ui:UiBinder> 